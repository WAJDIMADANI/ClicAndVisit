<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:layout_width="100dp"
        android:layout_height="@dimen/dimen_50dp"
        android:layout_marginBottom="@dimen/margin_global_20dp"
        android:background="@color/color_accent"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">
<!--        app:onClickWithDebounce="@{() ->viewModel.onMapsClicked()}"-->

        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="@dimen/dimen_24dp"
            android:layout_height="@dimen/dimen_22dp"
            android:layout_marginEnd="@dimen/padding_global_5dp"
            android:src="@drawable/ic_home_map" />

        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_global_2dp"
            android:fontFamily="@font/poppins_bold"
            android:text="@string/home_list"
            android:textColor="@color/white_basic"
            android:textSize="@dimen/dimen_16sp" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>

